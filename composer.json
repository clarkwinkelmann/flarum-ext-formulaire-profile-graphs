{
    "name": "clarkwinkelmann/flarum-ext-formulaire-profile-graphs",
    "description": "Formulaire profile fields as graphs",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.2",
        "kilowhat/flarum-ext-formulaire": "*"
    },
    "authors": [
        {
            "name": "Clark Winkelmann",
            "homepage": "https://clarkwinkelmann.com/",
            "email": "clark.winkelmann@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/clarkwinkelmann/flarum-ext-formulaire-profile-graphs"
    },
    "autoload": {
        "psr-4": {
            "ClarkWinkelmann\\FormulaireProfileGraphs\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Formulaire Profile Graphs",
            "category": "feature",
            "icon": {
                "name": "fas fa-chart-bar",
                "backgroundColor": "#684ba6",
                "color": "#fff"
            }
        }
    }
}
